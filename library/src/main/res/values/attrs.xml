<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TurnLoad">
        <!--最大幅度 180～360-->
        <attr name="maxRange" format="integer" />
        <!--圆环的宽度-->
        <attr name="strokeWidth" format="float" />
        <!--速度-->
        <attr name="speed" format="integer" />
        <!--背景色-->
        <attr name="backgroundColor" format="color" />
        <!--圆环颜色 格式"#182765,#789019"-->
        <attr name="turnColorList" format="string"/>
    </declare-styleable>

</resources>